{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:alchemunityIO/nova.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "3.7.2",
        "laravel/nova": "*",
        "laravel/passport": "^7.0",
        "laravel/scout": "^7.1",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "^1.0",
        "paypal/rest-api-sdk-php": "*",
        "predis/predis": "^1.1",
        "stripe/stripe-php": "^7.49",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Boss\\Test\\": "packages/boss/test/src",
            "Boss\\Login\\": "packages/boss/login/src"
        },
        "files": [
            "app/Helpers/global_function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "platform": {
        "ext-pcntl": "7.3",
        "ext-posix": "7.3"
      }
    }
}
